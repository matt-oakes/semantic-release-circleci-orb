{
  "name": "semantic-release-circleci-orb",
  "description": "semantic-release plugin to publish CircleCI Orbs",
  "version": "1.1.1",
  "author": "Matt Oakes (https://mattoakes.net)",
  "scripts": {
    "lint": "xo",
    "pretest": "npm run lint",
    "semantic-release": "semantic-release",
    "test": "nyc ava -v"
  },
  "bugs": {
    "url": "https://github.com/getoutreach/semantic-release-circleci-orb/issues"
  },
  "contributors": [
    "Matt Oakes <hello@mattoakes.net> (https://mattoakes.net)"
  ],
  "dependencies": {
    "@semantic-release/error": "^2.2.0",
    "aggregate-error": "^2.0.0",
    "execa": "^1.0.0",
    "npm": "^6.4.1",
    "read-pkg": "^5.0.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^7.0.8",
    "ava": "^1.0.1",
    "clear-module": "^3.0.0",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "dockerode": "^2.5.7",
    "fs-extra": "^7.0.1",
    "get-stream": "^5.0.0",
    "got": "^9.3.2",
    "mockserver-client": "^5.5.0",
    "nyc": "^13.1.0",
    "p-retry": "^4.0.0",
    "semantic-release": "^15.13.3",
    "sinon": "^7.1.1",
    "stream-buffers": "^3.0.2",
    "tempy": "^0.2.1",
    "xo": "^0.24.0"
  },
  "engines": {
    "node": ">=8.3"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "homepage": "https://github.com/getoutreach/semantic-release-circleci-orb#readme",
  "keywords": [
    "circleci",
    "circleci-orb",
    "publish",
    "registry",
    "semantic-release",
    "version"
  ],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "include": [
      "lib/**/*.js",
      "index.js"
    ],
    "reporter": [
      "json",
      "text",
      "html"
    ],
    "all": true
  },
  "peerDependencies": {
    "semantic-release": ">=15.9.0 <16.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/getoutreach/semantic-release-circleci-orb.git"
  },
  "xo": {
    "prettier": true,
    "space": true
  }
}
